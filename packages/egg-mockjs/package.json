{
  "name": "egg-mockjs",
  "version": "0.0.3",
  "description": "",
  "eggPlugin": {
    "name": "mockjs"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin"
  ],
  "dependencies": {
    "glob": "^7.1.6",
    "koa-is-json": "^1.0.0",
    "mockjs": "^1.1.0",
    "path-to-regexp": "^1.8.0"
  },
  "devDependencies": {
    "@mitod/eslint-config": "^0.0.6",
    "@types/node": "^12.12.7",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "babel-jest": "^24.9.0",
    "egg": "^2.16.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "eslint": "^6.1.0",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "tsc": "ets && tsc -p tsconfig.json",
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "egg-bin pkgfiles --check && npm run lint && npm run cov",
    "pkgfiles": "egg-bin pkgfiles",
    "autod": "autod",
    "fix": "eslint . --fix",
    "clean": "rimraf lib esm",
    "build:cj": "tsc --project tsconfig.json",
    "build:es": "tsc -m esNext --outDir esm",
    "build": "npm run clean && npm run build:cj && npm run build:es",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "app.js",
    "agent.js",
    "config/**/*.js",
    "app/**/*.js"
  ],
  "ci": {
    "version": "8, 10"
